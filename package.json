{
  "name": "test",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "lint": "node_modules/.bin/tslint '**/*.{ts,tsx}' -e '**/node_modules/**' -p 'tsconfig.json'",
    "test": "node scripts/test.js --env=jsdom"
  },
  "dependencies": {
    "autoprefixer": "7.1.6",
    "babel-core": "6.26.0",
    "babel-eslint": "7.2.3",
    "babel-loader": "7.1.2",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "classnames": "2.2.5",
    "css-loader": "0.28.7",
    "dotenv": "4.0.0",
    "dotenv-expand": "4.2.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.29.0",
    "node-sass": "4.7.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "raf": "3.4.0",
    "react": "16.2.0",
    "react-dev-utils": "5.0.0",
    "react-dom": "16.2.0",
    "react-router-dom": "4.2.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-manifest-plugin": "1.3.2",
    "whatwg-fetch": "2.0.3"
  },
  "devDependencies": {
    "@types/jest": "22.1.4",
    "@types/react": "16.0.38",
    "@types/react-dom": "16.0.4",
    "awesome-typescript-loader": "3.4.1",
    "babel-jest": "21.2.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "6.24.1",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "enzyme-to-json": "3.3.1",
    "identity-obj-proxy": "3.0.0",
    "jest": "22.4.1",
    "source-map-loader": "0.2.3",
    "ts-jest": "22.4.1",
    "tslint": "5.9.1",
    "tslint-consistent-codestyle": "1.11.1",
    "tslint-eslint-rules": "4.1.1",
    "tslint-react": "3.4.0",
    "typescript": "2.7.1"
  },
  "jest": {
    "clearMocks": true,
    "collectCoverageFrom": [
      "js/**/*.{js,jsx,ts,tsx,json}"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "jsx",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/js/config/styleMock.js"
    },
    "setupFiles": [
      "<rootDir>/js/config/polyfills.js"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/js/config/enzymeSetup.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)test)\\.(tsx?)$",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    }
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ]
  }
}
